{
  "name": "Head-up Github",
  "description" : "To use, type 'g' plus a search term into the Omnibox.",
  "version": "1.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "history",
    "bookmarks",
    "tabs"
  ],
  "omnibox": { "keyword" : "g" },
  "manifest_version": 2
}
